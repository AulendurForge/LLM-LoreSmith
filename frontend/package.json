{
  "name": "llm-loresmith-frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Frontend for LLM LoreSmith - A system for fine-tuning LLMs with trusted documents",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/nunito-sans": "^5.0.8",
    "@fontsource/oswald": "^5.0.18",
    "@fontsource/source-code-pro": "^5.0.16",
    "@reduxjs/toolkit": "^2.0.1",
    "@tanstack/react-query": "^5.8.7",
    "axios": "^1.6.2",
    "chart.js": "^4.4.0",
    "formik": "^2.4.5",
    "i18next": "^23.7.6",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-i18next": "^13.5.0",
    "react-icons": "^4.12.0",
    "react-redux": "^9.0.2",
    "react-router-dom": "^6.20.0",
    "react-toastify": "^9.1.3",
    "redux-persist": "^6.0.0",
    "uuid": "^11.1.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/detect-port": "^1.3.5",
    "@types/emscripten": "^1.40.0",
    "@types/node": "^20.17.24",
    "@types/normalize-package-data": "^2.4.4",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "storybook": "^7.5.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.8.2",
    "vite": "^5.0.2",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^0.34.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
